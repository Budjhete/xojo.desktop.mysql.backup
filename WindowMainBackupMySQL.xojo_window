#tag Events PushButton3
	#tag Event
		Sub Pressed()
		  if mMySQL_Backup.mDatabase <> Nil then
		    mMySQL_Backup.mDatabase.Close
		  end if
		  
		  
		  Dim db As New MySQLCommunityServer
		  db.Host = "localhost"
		  db.Port = 3306
		  db.DatabaseName = "k_test"
		  db.UserName = "test"
		  db.Password = "123"
		  
		  If db.Connect Then
		    db.ExecuteSQL("SET NAMES 'utf8'")
		    
		    mMySQL_Backup.mDatabase = db
		  Else
		    ' Connection error
		    MessageBox(db.ErrorMessage)
		  End If
		  
		  dim rc as RowSet = mMySQL_Backup.mDatabase.Tables
		  Listbox1.RemoveAllRows
		  
		  while not rc.AfterLastRow
		    Listbox1.AddRow(rc.ColumnAt(1).StringValue)
		    rc.MoveToNextRow
		  wend
		End Sub
	#tag EndEvent
